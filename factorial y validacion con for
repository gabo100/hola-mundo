#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;
void main()
{
	int factorial,i,n;
	factorial=1;
	do
	{
		cout<<"ingrese ele valor de n:";
		cin>>n;
	}
	while (n<0);// la condicion debe contemplar los valores
				//negar la condicion correcta
	for(i=2;i<=n;i++)
		factorial=factorial*i;
	cout<<"el factoria de"<<n<<"es"<<factorial;
getch();

}
